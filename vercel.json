{
    "version": 2,
    "name": "e-shop-tutorial",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "functions": {
        "controller/*.js": {
            "maxDuration": 50
        }
    }
}